{
    "manifest_version": 3,
    "name": "ThumbGrabber",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["activeTab", "scripting", "downloads"],
    "host_permissions": ["*://*.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    }
  }