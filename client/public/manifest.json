{
    "manifest_version": 3,
    "name": "ThumbGrabber",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["activeTab", "scripting", "downloads"],
    "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
    "host_permissions": ["*://*.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    }
  }